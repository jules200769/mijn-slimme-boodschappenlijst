{
  "expo": {
    "name": "check it!",
    "slug": "mijn-slimme-boodschappenlijst",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f29"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mijnslimmeboodschappenlijst.app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mijnslimmeboodschappenlijst.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f29"
      }
    },
    "scheme": "mijn-slimme-boodschappenlijst",
    "plugins": [
              [
          "expo-notifications",
          {
            "icon": "./assets/icon.png",
            "color": "#f29"
          }
        ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#f29"
    }
  }
}
